<?xml version="1.0" encoding="utf-8"?>
<scraper name="Adult DVD Empire" date="2020-06-01" content="movies" framework="1.0" thumb="gaydvdempire.jpg" language="en">
  <NfoUrl dest="3">
    <RegExp input="$$1" output="&lt;url&gt;http://www.\1&lt;/url&gt;" dest="3">
      <expression noclean="1">(gaydvdempire.com/\d+/\S+)</expression>
    </RegExp>
  </NfoUrl>
  <CreateSearchUrl dest="3">
    <RegExp input="$$1" output="&lt;url&gt;http://www.gaydvdempire.com/dvd/search?m=false&amp;amp;pageSize=100&amp;amp;view=list&amp;amp;q=\1&lt;/url&gt;" dest="3">
      <expression noclean="1"></expression>
    </RegExp>
  </CreateSearchUrl>
  <GetSearchResults dest="6">
    <RegExp input="$$5" output="&lt;?xml version=&quot;1.0&quot; encoding=&quot;iso-8859-1&quot; standalone=&quot;yes&quot;?&gt;&lt;results&gt;\1&lt;/results&gt;" dest="6">
      <RegExp input="$$1" output="&lt;entity&gt;&lt;title&gt;\2&lt;/title&gt;&lt;url&gt;http://www.gaydvdempire.com/\1?m=false&lt;/url&gt;&lt;/entity&gt;" dest="5">
        <expression repeat="yes" trim="1">&lt;a href="/([^"]*)"[^&gt;]* Label="Title"&gt;([^&lt;]+)&lt;/a&gt;</expression>
      </RegExp>
      <expression noclean="1"></expression>
    </RegExp>
  </GetSearchResults>
  <GetDetails dest="3">
    <RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="3">
      <RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="5+">
        <expression>&lt;a data-href="([^"]*[0-9]+h\.jpg)"[^&gt;]*id="front-cover"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="5+">
        <expression>&lt;a href="([^"]*[0-9]+bh\.jpg)"[^&gt;]*id="back-cover"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;title&gt;\1&lt;/title&gt;" dest="5+">
        <expression trim="1">&lt;h1&gt;\s*(.*?)(\s{2,}.*)?&lt;/h1&gt;</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;studio&gt;\1&lt;/studio&gt;" dest="5+">
        <expression trim="1">Label="Studio - Details"&gt;([^&lt;]+)</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;year&gt;\1&lt;/year&gt;" dest="5+">
        <expression trim="1">Production Year:.?&lt;/small&gt;([^&lt;]+)</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;tagline&gt;\1&lt;/tagline&gt;" dest="5+">
        <expression>&lt;a name="tagline".*?&lt;h2[^&gt;]*&gt;([^&lt;]+)&lt;/h2&gt;</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;plot&gt;\1&lt;/plot&gt;" dest="5+">
        <expression trim="1">&lt;h4 class="m-b-0 text-dark synopsis"&gt;&lt;p&gt;([^&lt;]+)&lt;/p&gt;&lt;/h4&gt;</expression>
      </RegExp>
      <RegExp input="$$7" output="&lt;actor&gt;&lt;name&gt;\2&lt;/name&gt;&lt;thumb&gt;\1.jpg&lt;/thumb&gt;&lt;/actor&gt;" dest="5+">
        <RegExp input="$$1" output="\1" dest="7">
          <expression clear="yes" noclean="1">&lt;div class="cast-headshots"&gt;&lt;ul[^&gt;]*&gt;(.*?)&lt;/ul&gt;</expression>
        </RegExp>
        <expression repeat="yes" trim="1">style="background-image:url.([^)]+)t\.jpg.[^&gt;]*&gt;&lt;span&gt;([^&lt;]+)</expression>
      </RegExp>
      <RegExp input="$$7" output="&lt;actor&gt;&lt;name&gt;\1&lt;/name&gt;&lt;/actor&gt;" dest="5+">
        <RegExp input="$$1" output="\1" dest="7">
          <expression clear="yes" noclean="1">Cast&lt;/h2&gt;&lt;ul[^&gt;]*&gt;(.*?)&lt;/ul&gt;</expression>
        </RegExp>
        <expression repeat="yes" trim="1">Label="Performers - detail"[^&gt;]*&gt;([^&lt;]+)</expression>
      </RegExp>
      <RegExp input="$$7" output="&lt;genre&gt;\1&lt;/genre&gt;" dest="5+">
        <RegExp input="$$1" output="\1" dest="7">
          <expression clear="yes" noclean="1">&lt;div class="m-b-1"&gt;&lt;strong&gt;Categories:&lt;/strong&gt;([^!)]+)</expression>
        </RegExp>
        <expression repeat="yes" trim="1">Label="Category"&gt;([^&lt;]+)</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;runtime&gt;\1&lt;/runtime&gt;" dest="5+">
        <expression trim="1">Length:.?&lt;/small&gt;([^&lt;]+)</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;mpaa&gt;\1&lt;/mpaa&gt;" dest="5+">
        <expression trim="1">Rating:.?&lt;/small&gt;([^&lt;]+)</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;rating max=&quot;5&quot;&gt;\1&lt;/rating&gt;" dest="5+">
        <expression>&lt;span class="rating-stars-avg"&gt;(\d\.\d{2})&lt;/span&gt;</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;director&gt;\1&lt;/director&gt;" dest="5+">
        <expression repeat="yes" trim="1">Label="Director - details"&gt;([^&lt;]*)</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;set&gt;\1&lt;/set&gt;" dest="5+">
        <expression trim="1">Label="Series"[^&gt;]*&gt;\s*"([^"]+)"</expression>
      </RegExp>
      <RegExp input="$$1" output="&lt;trailer&gt;\1&lt;/trailer&gt;" dest="5+">
        <expression>&lt;source src="(.*?)"\s+type="video\/mp4"\s+data-default="true"&gt;&lt;\/video&gt;</expression>
      </RegExp>
      <RegExp input="$$8" output="&lt;fanart&gt;$$8&lt;/fanart&gt;" dest="5+">
        <RegExp input="$$7" output="&lt;thumb&gt;\1&lt;/thumb&gt;" dest="8">
          <RegExp input="$$1" output="\1" dest="7">
            <expression clear="yes" noclean="1">&lt;div class="row scene-row(.*?)&gt;Product Information&lt;</expression>
          </RegExp>
          <expression repeat="yes" clear="yes">&lt;a rel="screenshots"\s+href="(.*?)"</expression>
        </RegExp>
        <expression></expression>
      </RegExp>
      <expression noclean="1"></expression>
    </RegExp>
  </GetDetails>
</scraper>